{
  "name": "nestjs-cms-discovery-fastify",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "nest build --all",
    "start:cms": "nest start cms --watch",
    "start:discovery": "nest start discovery --watch",
    "start:prod:cms": "node dist/apps/cms/main.js",
    "start:prod:discovery": "node dist/apps/discovery/main.js",
    "typeorm:run": "ts-node -r tsconfig-paths/register libs/database/src/datasource.ts migration:run",
    "typeorm:gen": "ts-node -r tsconfig-paths/register libs/database/src/datasource.ts migration:generate -d libs/database/src/migrations -n Auto",
    "lint": "eslint ."
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.8",
    "@nestjs/platform-fastify": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.4.8",
    "@types/node": "^20.14.11",
    "eslint": "^9.8.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
